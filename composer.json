	"format": "@php ./vendor/squizlabs/php_codesniffer/bin/phpcbf"
  },
  "support"    : {
	"issues": "https://github.com/khoipro/wpstarter/issues",
	"source": "https://github.com/khoipro/wpstarter"
  }
}
